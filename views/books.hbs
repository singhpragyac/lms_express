                {{#each data}}
                    {{this.TITLE}}<br>
                {{/each}}  
            </td>   
              <td> 
                {{#each data}}
                    {{this.AUTHOR_ID}}<br>
                {{/each}}  
            </td>   
              <td> 
                {{#each data}}
                    {{this.ISBN}}<br>
                {{/each}}  
            </td>   
              <td> 
                {{#each data}}
                    {{this.PUBLICATION_YEAR}}<br>
                {{/each}}  
            </td>   
              <td> 
                {{#each data}}
                    {{this.GENRE}}<br>
                {{/each}}  
            </td>   
            <td> 
                {{#each data}}
                    {{this.CREATED_ON}}<br>
                {{/each}}  
